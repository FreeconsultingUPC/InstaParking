<%= simple_form_for(@place) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :url_image %>
    <%= f.input :state %>
    <%= f.input :started_at %>
    <%= f.input :ended_at %>
    <%= f.input :started_time %>
    <%= f.input :ended_time %>
    <%= f.input :description %>
    <%= f.association :users %>
    <%= f.association :districts %>
    <%= f.association :reservation_types %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
